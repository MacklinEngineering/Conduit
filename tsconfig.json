{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "noImplicitAny": false,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "Node16",
    "types": ["node"],
    "target": "es2022",
    "module": "node16"
  },
  "ts-node": {
    "esm": true
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
